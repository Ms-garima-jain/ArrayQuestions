// Find average of even numbers.
 const arr = [1, 2, 3, 4, 4, 5]
  let sum=0
  let count=0
 for ( let i=0; i<arr.length; i++)
 {
     if (arr[i]%2==0)
     {
         sum+=arr[i]
          count++
     }
 }
 if (count>0)
 {
     console.log (sum/count)
 }
  else
  {
     console.log (" no even numbers")
  }
