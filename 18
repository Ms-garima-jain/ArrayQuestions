let arr = [1,2,3,4,5,6, 7];
let evencount=0;
let oddcount=0;
 for (let i =0; i<arr.length; i++)
 {
      if (arr[i]%2==0)
      {
          evencount++
      }
      else
      {
          oddcount++
      }
 }
  if (evencount>oddcount)
  {
      console.log ("even count more")
  }
  else if (evencount<oddcount)
  {
       console.log ("odd count more")
  }
  else
  {
       console.log ("both are equal")
  }
