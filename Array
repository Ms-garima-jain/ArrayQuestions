// Count number of even and odd elements in an array.

 const arr = [1, 2, 3, 4, 5, 6, 7, 8]
 let evencount=0;
  let oddcount=0;
   for ( let i =0; i<arr.length; i++)
   {
     if (arr[i]%2==0)
        evencount++
    else
        oddcount++
   }
    console.log ("evencount"+ evencount)
     console.log ("oddcount"+ oddcount)
