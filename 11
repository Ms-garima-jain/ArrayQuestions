let arr =[1,2,3,4,5,6]
let even=[]
let odd =[]
let eindex=0;
let oindex=0
 for ( let i=0; i<arr.length ; i++)
 {
      if (arr[i]%2==0)
      {
          even[eindex]=arr[i]
          eindex++
      }
      else
      {
          odd[oindex]=arr[i]
          oindex++
      }
 }
  console.log (even)
   console.log (odd)
